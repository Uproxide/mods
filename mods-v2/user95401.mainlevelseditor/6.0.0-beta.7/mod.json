{
	"geode": "2.0.0-beta.25",
	"gd": {
		"win": "2.204",
		"android": "*"
	},
	"version": "v6.0.0-beta.7",
	"id": "user95401.mainlevelseditor",
	"name": "Main Levels Editor",
	"developer": "user95401",
	"description": "customizable thing to change main levels uknowya",
	"repository": "https://github.com/user95401/MainLevelsEditor",
	"tags": [ "Interface", "Utility", "Customization", "Cheats" ],
	"settings": {
		"SGG": {
			"name": "Stat Gain Guard",
			"description": "ignore stars if its over max value",
			"type": "bool",
			"default": true
		},
		"SGGS": {
			"name": "^ SGG Stars:",
			"description": "ignore stars if its over max value",
			"type": "int",
			"min": 0,
			"default": 22
		},
		"SL": {
			"name": "Saved levels (BETA)",
			"description": "<cr>experimental function</c> that allows u replace stuff on online levels",
			"type": "bool",
			"default": false
		},
		"UI": {
			"name": "Add control UI",
			"description": "add buttons an other stuff to control things in game",
			"type": "bool",
			"default": false
		},
		"USE_PATCHES": {
			"name": "Use patches (PC Only)",
			"description": "<cr>this thing make dont working NO_THETOWER, NO_COMINGSOON, COMINGSOON_TEXT keys\nalso this is for windows only",
			"type": "bool",
			"default": false
		},
		"NO_THETOWER": {
			"name": "Remove The Tower Page",
			"description": "dont add this page yea",
			"type": "bool",
			"default": false
		},
		"NO_COMINGSOON": {
			"name": "Remove Coming Soon Page",
			"description": "nothin coming",
			"type": "bool",
			"default": false
		},
		"COMINGSOON_TEXT": {
			"name": "Text for \"Coming Soon\" Page",
			"description": "nothin coming",
			"type": "string",
			"default": "Coming Soon!"
		},
		"OVERWRITE_COLOR": {
			"name": "Main color for all pages",
			"description": "use main color only",
			"type": "bool",
			"default": false
		},
		"MAIN_COLOR": {
			"name": "Main color",
			"type": "rgb",
			"default": [ 130, 130, 190 ]
		},
		"BG_SPRITE_NAME": {
			"name": "Background FILE NAME",
			"type": "file",
			"default": "GJ_gradientBG.png"
		},
		"STRETCH_BG": {
			"name": "Stretch the background",
			"type": "bool",
			"default": true
		},
		"BG_Y": {
			"name": "Background Y point",
			"type": "float",
			"default": 0.13,
			"max": 1,
			"min": -1,
			"one-of": 0.001
		},
		"SHOW_GROUND": {
			"name": "Show ground",
			"type": "bool",
			"default": true
		},
		"GROUND_ID": {
			"name": "Ground",
			"type": "int",
			"default": 1,
			"max": 127,
			"min": 1
		},
		"GROUND_LINE": {
			"name": "Ground line",
			"type": "int",
			"default": 1,
			"max": 3,
			"min": 1
		}
	}
}
